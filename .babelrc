{
	"plugins": [
		"@babel/plugin-proposal-class-properties"
	],
	"presets": [
		"@babel/react",
		"@babel/flow"
	],
	"env": {
		"es": {
			"presets": [
				["@babel/env", {
					"modules": false,
					"useBuiltIns": false
				}]
			]
		},
		"commonjs": {
			"presets": [
				["@babel/env", {
					"modules": "commonjs",
					"useBuiltIns": false
				}]
			]
		}
	}
}
