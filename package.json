{
  "name": "react-t7e",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "es": "./es/index.js",
  "repository": "git@github.com:woutervanvliet/react-t7e.git",
  "author": "Wouter M. van Vliet <wouter@interpotential.com>",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "react",
    "i18n",
    "translate",
    "reactjs",
    "translations",
    "gettext",
    "mo",
    "po"
  ],
  "scripts": {
    "lint": "eslint --ignore-pattern=!.eslintrc.js .eslintrc.js src/",
    "clean": "rimraf es/ lib/",
    "prepare": "yarn build",
    "prebuild": "yarn run clean",
    "build": "yarn build:babel && yarn build:flow",
    "build:babel": "yarn build:babel:es && yarn build:babel:lib",
    "build:babel:es": "BABEL_ENV=es babel src -d es",
    "build:babel:lib": "BABEL_ENV=commonjs babel src -d lib",
    "build:flow": "yarn build:flow:lib && yarn build:flow:es",
    "build:flow:lib": "flow-copy-source src lib",
    "build:flow:es": "flow-copy-source src es"
  },
  "files": [
    "lib/",
    "es",
    "src"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-flow": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "babel-eslint": "^8.2.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.77.0",
    "flow-copy-source": "^2.0.2",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^16.3.0-0"
  },
  "dependencies": {
    "jed": "^1.1.1",
    "jed-gettext-parser": "^1.0.0"
  }
}
