{
  "name": "react-t7e",
  "version": "0.0.7",
  "main": "./lib/index.js",
  "es": "./es/index.js",
  "types": "react-t7e.d.ts",
  "repository": "git@github.com:woutervanvliet/react-t7e.git",
  "author": "Wouter M. van Vliet <wouter@interpotential.com>",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "react",
    "i18n",
    "translate",
    "reactjs",
    "translations",
    "gettext",
    "mo",
    "po"
  ],
  "scripts": {
    "test": "BABEL_ENV=commonjs mocha",
    "posttest": "yarn lint",
    "lint": "eslint --ignore-pattern=!.eslintrc.js .eslintrc.js src/",
    "clean": "rimraf es/ lib/",
    "prepare": "yarn build",
    "prebuild": "yarn run clean",
    "build": "yarn build:babel && yarn build:flow",
    "build:babel": "yarn build:babel:es && yarn build:babel:lib",
    "build:babel:es": "BABEL_ENV=es babel src -d es",
    "build:babel:lib": "BABEL_ENV=commonjs babel src -d lib",
    "build:flow": "yarn build:flow:lib && yarn build:flow:es",
    "build:flow:lib": "flow-copy-source src lib",
    "build:flow:es": "flow-copy-source src es"
  },
  "files": [
    "react-t7e.d.ts",
    "lib/",
    "es",
    "src"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/react": "^16.4.14",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.81.0",
    "flow-copy-source": "^2.0.2",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": "^16.3.0-0"
  },
  "dependencies": {
    "jed": "^1.1.1",
    "jed-gettext-parser": "^1.0.0"
  }
}
