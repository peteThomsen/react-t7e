{
  "name": "react-t7e",
  "version": "0.1.2",
  "main": "./lib/index.js",
  "es": "./es/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:woutervanvliet/react-t7e.git",
  "author": "Wouter M. van Vliet <wouter@interpotential.com>",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "react",
    "i18n",
    "translate",
    "reactjs",
    "translations",
    "gettext",
    "mo",
    "po"
  ],
  "scripts": {
    "test": "BABEL_ENV=commonjs mocha",
    "lint": "eslint --ignore-pattern=!.eslintrc.js .eslintrc.js dist/",
    "clean": "rimraf es/ lib/",
    "prepare": "yarn build",
    "prebuild": "yarn run clean",
    "build": "yarn build:es6 && yarn build:es5",
    "build:es6": "tsc --target es6 --module esnext --outDir ./es",
    "build:es5": "tsc --target es5 --module commonjs --outDir ./lib"
  },
  "files": [
    "react-t7e.d.ts",
    "lib/",
    "es/",
    "src/"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/react": "^16.4.14",
    "babel-eslint": "^9.0.0",
    "chai": "^4.1.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react": "^16.3.0-0"
  },
  "dependencies": {
    "jed": "^1.1.1",
    "jed-gettext-parser": "^1.0.0"
  }
}
